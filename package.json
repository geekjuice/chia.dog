{
  "name": "chia-dog",
  "version": "1.0.0",
  "description": "Hello, Chia!",
  "license": "MIT",
  "repository": "geekjuice/chia-dog",
  "author": {
    "name": "Nick Hwang",
    "email": "nick.joosung.hwang@gmail.com"
  },
  "keywords": [
    "bottle",
    "backend",
    "chia-dog"
  ],
  "eslintConfig": {
    "extends": "geekjuice"
  },
  "ava": {
    "files": [
      "__tests__/**/*.spec.js"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "reinstall": "del node_modules && npm install",
    "lint": "eslint lib",
    "test": "ava --verbose",
    "test:watch": "ava --verbose --watch",
    "clean": "del dist",
    "build": "babel --copy-files lib --out-dir dist",
    "clean+build": "npm run clean && npm run build",
    "tunnel": "lt --subdomain chia-dog --port 3000",
    "server": "babel-node lib/main.js",
    "watch": "nodemon --watch lib --exec npm run server",
    "start": "DEBUG=chia-dog:* npm run watch",
    "debug": "DEBUG=chia-dog:* iron-node lib/debug.js",
    "deploy": "./bin/deploy"
  },
  "dependencies": {
    "cron": "^1.1.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "forever": "^0.15.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.0.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.0.1",
    "moment": "^2.12.0",
    "promise": "^7.1.1",
    "radium": "^0.17.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "rxjs": "^5.0.0-beta.5"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "chalk": "^1.1.3",
    "del-cli": "^0.2.0",
    "eslint": "^2.7.0",
    "eslint-config-geekjuice": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.3.0",
    "nodemon": "^1.9.1"
  }
}
